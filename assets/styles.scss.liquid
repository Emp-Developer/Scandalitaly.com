$table-background-color: #f9f9f9;
$table-border-color: #dcdcdc;

table {
  width: 100%;
  margin-bottom: 20px;

  th {
    font-weight: 700;
    text-align: left;
    background-color: $table-background-color;
  }

  th,
  td {
    padding: 4px;
    border-right: 1px solid $table-border-color;

    &:first-child {
      border-left: 1px solid $table-border-color;
    }
  }

  tr {
    border-bottom: 1px solid $table-border-color;

    &:first-child {
      border-top: 1px solid $table-border-color;
    }

    .order_summary td.label {
      text-align: right;
    }
  }
}

{% unless settings.table_styles_enabled %}
  table {

    th {
      background-color: transparent;
    }

    th,
    td,
    tr {
      border: none;

      &:first-child {
        border: none;
      }
    }
  }
{% endunless %}

/* Responsive order tables */

.collapsible-table {
  @include respond-to('medium') {
    thead {
      display: none;
    }

    tr {
      display: block;
      margin-bottom: 20px;
      border: 1px solid $table-border-color;

      &:last-child {
        margin-bottom: 0;
      }
    }

    td {
      display: block;
      @include flexbox();
      @include align-items(center);
      @include justify-content(space-between);
      padding: 0 10px 0 0;
      border: none;
      border-bottom: 1px solid $table-border-color;

      &:before {
        content: attr(data-label);
        min-width: 50%;
        font-weight: bold;
        padding-top: 4px;
        padding-bottom: 4px;
        text-indent: 10px;
        border-right: 1px solid $table-border-color;
        background-color: $table-background-color;
      }

      &:last-child {
        border-bottom: none;
      }
    }

    tfoot {
      display: block;
      margin-top: 20px;

      tr {
        margin-bottom: 0;
        border-bottom: none;

        &:last-child {
          border-bottom: 1px solid $table-border-color;
        }
      }
    }
  }
}

