      @include object-fit(cover);
    }
  }
  .mapLayout--info {
    @include flexbox();
    @include align-items(center);
    width: 25%;
    max-width: 350px;
    height: 300px;
    position: absolute;
    padding: 20px 40px;
    top: 30px;
    border: 1px solid {{ settings.border_color }};
    box-shadow: rgba(0, 0, 0, 0.3) 0px 1px 4px -1px;
    &.info--right {
      right: 10%;
    }
    &.info--left {
      left: 10%;
    }

    @include respond-to('medium') {
      position: relative;
      width: calc(100% - 80px);
      max-width: 100%;
      border-color: none;
      box-shadow: none;
      top: 0;
      &.info--right {
        right: 0%;
      }
      &.info--left {
        left: 0%;
      }
    }

    .info {
      width: 100%;
    }

    p {
      margin-bottom: 0;
    }
    .action_button {
      margin-top: 20px;
    }
  }
