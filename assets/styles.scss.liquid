/* #Videos
================================================== */
.video-wrapper {
  position: relative;
  background-color: #000000;

  .plyr--video,
  .plyr__video-embed,
  video {
    z-index: -1;
    opacity: 0;
  }

  &.video-is-playing {
    .plyr--video,
    .plyr__video-embed,
    video {
      z-index: 1;
      opacity: 1;
    }
  }

  .video__overlay {
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    margin-left: auto;
    margin-right: auto;
    z-index: 1;
  }

  &.darken-video--true {
    .video__overlay {
      background: rgba(0,0,0,0.3);
    }
  }

  .plyr--full-ui.plyr--video .plyr__control--overlaid {
    display: none;
  }

  &.play-button-icon--visible {
    .plyr--paused .plyr__control--overlaid {
      display: block;
      opacity: 1;
    }
  }

  .video-wrapper__image {
